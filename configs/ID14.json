{
    "$schema": "https://raw.githubusercontent.com/KonstantinDege/schemas/refs/heads/main/mission_schema.json",
    "parameter": {
        "flight_height": 2
    },
    "commands": [
        {
            "action": "takeoff"
        },
        {
            "action": "start_camera",
            "commands": {
                "ips": 1
            }
        },
        {
        "action": "mov_local",
        "commands": [
            {
                "x": 2.0,
                "y": 2.0,
                "z": -2.0,
                "yaw": 90.0
            }
        ]
        },
        {
            "action": "delay",
            "commands": {
                "time": 2
            }
        },
        {
        "action": "mov_local",
        "commands": [
            {
                "x": -2.0,
                "y": -2.0,
                "z": -2.0,
                "yaw": 270.0
            }
        ]
        },
        {
            "action": "delay",
            "commands": {
                "time": 2
            }
        },
        {
        "action": "mov_local",
        "commands": [
            {
                "x": 0,
                "y": 0,
                "z": -2.0,
                "yaw": 0.0
            }
        ]
        },
        {
            "action": "stop_camera"
        },
        {
            "action": "delay",
            "commands": {
                "time": 2
            }
        },
        {
            "action": "land_at"
        }
    ]
}